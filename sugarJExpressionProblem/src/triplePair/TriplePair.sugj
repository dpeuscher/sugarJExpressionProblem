package triplePair;

public class TriplePair<A, B, C> {
  public A a;
  public B b;
  public C c;
  
  public TriplePair (A a, B b,C c) {
    this.a = a;
    this.b = b;
    this.c = c;
  }
   
  public static <A, B, C> TriplePair<A, B, C> create(A a, B b,C c) {
    return new TriplePair(a, b, c);
  }
  
  public String toString() {
    return "(" + a.toString() + ", " + b.toString() + ", " + c.toString() + ")";
  }
}
