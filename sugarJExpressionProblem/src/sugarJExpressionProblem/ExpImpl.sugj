package sugarJExpressionProblem;

import sugarJExpressionProblem.Sugar;
import staticExpressionProblem.IPrint;

family ExpImpl {
	virtual class Int {
		public IPrint print (final int x) {
			return new IPrint() {
				public String print() {
					return new Integer(x).toString();
				}
			};
		}
	}
}
