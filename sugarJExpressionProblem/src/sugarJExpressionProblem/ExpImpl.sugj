package sugarJExpressionProblem;

import sugarJExpressionProblem.Sugar;

exp ExpImpl {
	entity Lit;
	entity Add;
	method IPrint print {
		public Lit(final int x) {
			return new IPrint() {
				public String print() {
					return new Integer(x).toString();
				}
			};
		}
		public add(final IPrint e1, final IPrint e2) {
		return new IPrint() {
			public String print() {
				return e1.print() + " + " + e2.print();
			}
		};
		
	}
}