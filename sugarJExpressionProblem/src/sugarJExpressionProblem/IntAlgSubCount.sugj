package sugarJExpressionProblem;

import sugarJExpressionProblem.Sugar;

family IntAlgAddCount extends IntAlgAdd,IntAlgCount {
	virtual class Add (IntAlgAddCount e1,IntAlgAddCount e2) {
		public Integer count (Integer add) { /* int durch Tree ersetzen*/
			return new Integer(add + e1.count() + e2.count());
		}
	}
}
